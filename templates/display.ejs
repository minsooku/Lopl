<!DOCTYPE html>
<html>
<head>
    <title>Favorite Places</title>
</head>
<body>
    <h1>My Favorite Places</h1>
    <% favoritePlaces.forEach(function(place) { %>
        <div>
            <h2><%= place.title %></h2> <!-- Replace 'title' with the actual key in your place objects -->
            <p><%= place.category %></p> <!-- Replace 'description' with the actual key in your place objects -->
            <p><%= place.description %></p> <!-- Replace 'location' with the actual key in your place objects -->
            <hr>
        </div>
    <% }); %>
</body>
</html>
